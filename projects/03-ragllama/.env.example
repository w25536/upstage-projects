# LLM Configuration
UPSTAGE_API_KEY=your_upstage_api_key_here

# Web Search Configuration
TAVILY_API_KEY=your_tavily_api_key_here

# LangSmith 연동을 위한 환경 변수 추가
LANGCHAIN_TRACING_V2="true"
LANGCHAIN_API_KEY="lsv2_..." 
LANGCHAIN_PROJECT="headhunter-ai-agent" # LangSmith 프로젝트 이름

# Embedding Configuration 
EMBEDDING_MODEL=jhgan/ko-sroberta-multitask
EMBEDDING_DEVICE=cpu

# PostgreSQL Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=headhunter_db
DB_USER=headhunter_user
DB_PASSWORD=headhunter_pass
DB_URL=postgresql://headhunter_user:headhunter_pass@localhost:5432/headhunter_db

# Vector Store Configuration
VECTOR_STORE_PATH=./vector_store
KNOWLEDGE_DATA_PATH=./data

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost